{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"]
        },
        "allowJs": true,
        "typeRoots": ["./types", "./node_modules/@types"],
        "types": ["markdown-it"]
    },
    "exclude": ["node_modules", "**/node_modules/*", ".vscode", "dist"],
    "typescript.inlayHints.parameterNames.enabled": "all"
}
